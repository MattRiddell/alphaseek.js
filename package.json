{
  "name": "alphaseek",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript library for Alphaseek SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jochasinga/alphaseek.js.git"
  },
  "keywords": [
    "Alphaseek",
    "referral"
  ],
  "author": "Joe Chasinga <joe@alphaseek.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jochasinga/alphaseek.js/issues"
  },
  "homepage": "https://github.com/jochasinga/alphaseek.js#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
