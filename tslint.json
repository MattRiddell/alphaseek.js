{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": [true, {
      "import-sources-order": "any",
      "named-imports-order": "case-insensitive"
    }],
    "variable-name": [true,
        "ban-keywords",
        "check-format",
        "allow-leading-underscore"
    ]
  }
}